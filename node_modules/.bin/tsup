#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules/tsup/dist/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules/tsup/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules/tsup/dist/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules/tsup/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/tsup@8.4.0_@swc+core@1.11.9_@swc+helpers@0.5.15__jiti@2.4.2_postcss@8.5.3_tsx@4.19.3_typescript@5.8.2_yaml@2.7.0/node_modules:/mnt/c/WEB_PROJECTS/withseismic/withseismic_com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
